            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/dimensionless"
            android:textSize="30sp" />

        <Button
            android:id="@+id/button_electron_proton_mass_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_proton_mass_ratio" />

        <Button
            android:id="@+id/button_number_of_particles_in_debye_sphere"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/number_of_particles_in_debye_sphere" />

        <Button
            android:id="@+id/button_alfven_velocity_speed_of_light"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/alfven_velocity_speed_of_light" />

        <Button
            android:id="@+id/button_electron_plasma_gyrofrequency_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_plasma_gyrofrequency_ratio" />

        <Button
            android:id="@+id/button_ion_plasma_gyrofrequency_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_plasma_gyrofrequency_ratio" />

        <Button
            android:id="@+id/button_thermal_magnetic_energy_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/thermal_magnetic_energy_ratio" />

        <Button
            android:id="@+id/button_magnetic_ion_rest_energy_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/magnetic_ion_rest_energy_ratio" />

        <TextView
            android:id="@+id/textView_miscellaneous"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:fontFamily="sans-serif-black"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/miscellaneous"
            android:textSize="30sp" />

        <Button
            android:id="@+id/button_bohm_diffusion_coefficient"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/bohm_diffusion_coefficient" />

        <Button
            android:id="@+id/button_transverse_spitzer_resistivity"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/transverse_spitzer_resistivity" />

