<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/frequencies_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:fontFamily="sans-serif-black"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/frequencies"
            android:textSize="30sp" />

        <Button
            android:id="@+id/electron_gyrofrequency_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_gyrofrequency" />

        <Button
            android:id="@+id/ion_gyrofrequency_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_gyrofrequency" />

        <Button
            android:id="@+id/electron_plasma_frequency_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_plasma_frequency" />

        <Button
            android:id="@+id/ion_plasma_frequency_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_plasma_frequency" />

        <Button
            android:id="@+id/electron_trapping_rate_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_trapping_rate" />

        <Button
            android:id="@+id/ion_trapping_rate_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_trapping_rate" />

        <Button
            android:id="@+id/electron_collision_rate_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_collision_rate" />

        <Button
            android:id="@+id/ion_collision_rate_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_collision_rate" />

        <TextView
            android:id="@+id/lengths_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:fontFamily="sans-serif-black"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/lengths"
            android:textSize="30sp" />

        <Button
            android:id="@+id/electron_debroglie_length_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_debroglie_length" />

        <Button
            android:id="@+id/classical_distance_of_minimum_approach_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/classical_distance_of_minimum_approach" />

        <Button
            android:id="@+id/electron_gyroradius_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_gyroradius" />

        <Button
            android:id="@+id/ion_gyroradius_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_gyroradius" />

        <Button
            android:id="@+id/electron_inertial_length_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_inertial_length" />

        <Button
            android:id="@+id/ion_inertial_length_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_inertial_length" />

        <Button
            android:id="@+id/debye_length_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/debye_length" />

        <TextView
            android:id="@+id/velocities_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:fontFamily="sans-serif-black"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/velocities"
            android:textSize="30sp" />

        <Button
            android:id="@+id/electron_thermal_velocity_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_thermal_velocity" />

        <Button
            android:id="@+id/ion_thermal_velocity_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_thermal_velocity" />

        <Button
            android:id="@+id/ion_sound_velocity_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_sound_velocity" />

        <Button
            android:id="@+id/alfven_velocity_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/alfven_velocity" />

        <TextView
            android:id="@+id/dimensionless_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:fontFamily="sans-serif-black"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/dimensionless"
            android:textSize="30sp" />

        <Button
            android:id="@+id/electron_proton_mass_ratio_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_proton_mass_ratio" />

        <Button
            android:id="@+id/number_of_particles_in_debye_sphere_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/number_of_particles_in_debye_sphere" />

        <Button
            android:id="@+id/alfven_velocity_speed_of_light_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/alfven_velocity_speed_of_light" />

        <Button
            android:id="@+id/electron_plasma_gyrofrequency_ratio_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/electron_plasma_gyrofrequency_ratio" />

        <Button
            android:id="@+id/ion_plasma_gyrofrequency_ratio_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/ion_plasma_gyrofrequency_ratio" />

        <Button
            android:id="@+id/thermal_magnetic_energy_ratio_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/thermal_magnetic_energy_ratio" />

        <Button
            android:id="@+id/magnetic_ion_rest_energy_ratio_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/magnetic_ion_rest_energy_ratio" />

        <TextView
            android:id="@+id/miscellaneous_textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:fontFamily="sans-serif-black"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/miscellaneous"
            android:textSize="30sp" />

        <Button
            android:id="@+id/bohm_diffusion_coefficient_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:text="@string/bohm_diffusion_coefficient" />

        <Button
            android:id="@+id/transverse_spitzer_resistivity_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="80dp"
            android:text="@string/transverse_spitzer_resistivity" />

    </LinearLayout>

</ScrollView>